pB;QACAM,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACAZ,IAAI,CAAC,aAAD,CAAJ;MACD,CAVD,EAWCe,KAXD,CAWO,MAAI;QACTf,IAAI,CAAC,cAAD,CAAJ;MACD,CAbD;IAcD,CA3CK;;IA4CNuB,KAAK,CAACH,IAAD,EAAM;MACTrB,KAAK,CAACyB,MAAN,CAAa,uCAAuCJ,IAAI,CAACjB,GAAzD,EACCQ,IADD,CACM,MAAI;QACR,KAAKF,IAAL;QACA,KAAKP,IAAL,CAAUE,QAAV,GAAqB,EAArB;QACAJ,IAAI,CAAC,eAAD,CAAJ;MACD,CALD,EAMCe,KAND,CAMO,MAAI;QACTf,IAAI,CAAC,YAAD,CAAJ;MACD,CARD;IASF;;EAtDM,CAbK;;EAsEbyB,OAAO,GAAG;IACR,KAAKhB,IAAL;EACF;;AAxEa,CAAf","names":["axios","swal","data","form","_id","ItemName","__v","users","updateSubmit","methods","load","get","then","res","console","log","catch","alert","save","post","edit","user","update","put","hapus","delete","mounted"],"sourceRoot":"","sources":["D:\\api\\src\\App.vue"],"sourcesContent":["<template lang=\"html\">\n<link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT\" crossorigin=\"anonymous\">\n<div class=\"\">\n  <h1 class=\"text-center\">TUGAS MEMBUAT CRUD PROYEK 3</h1>\n  <form @submit.prevent=\"save\">\n    <input type=\"text\" v-model=\"form._id\" name=\"_id\" >\n    <input type=\"text\" class=\"form-control\" placeholder=\"Input nama\" v-model=\"form.ItemName\" name=\"ItemName\" ><br>\n    <button type=\"submit\" v-show=\"!updateSubmit\" name=\"button\" class=\"btn btn-primary\">Save</button>\n    <button type=\"button\" v-show=\"updateSubmit\" v-on:click=\"update(form)\" name=\"button\" class=\"btn btn-primary\">Update</button>\n  </form>\n\n<br><h4 class=\"text-center\">DATA USERS</h4></div>\n\n<ul v-for=\"user in users\" :key=\"user.id\">\n  <table class=\"table\">\n  <thead>\n    <tr>\n      <th scope=\"col\">ID</th>\n      <th scope=\"col\">Name</th>\n      <th scope=\"col\">Action</th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr>\n      <td>\n        <li>{{ user._id }} <br>\n        </li>\n      </td>\n      <td>\n        <li>{{ user.ItemName }} <br>\n        </li>\n      </td>\n      <td>\n        <li><button type=\"button\" v-on:click=\"edit(user)\" name=\"button\" class=\"btn btn-success\">Edit</button> ||\n        <button type=\"button\" v-on:click=\"hapus(user)\" name=\"button\" class=\"btn btn-danger\">Delete</button>\n        </li>\n      </td>\n    </tr>\n  </tbody>\n</table>\n</ul>\n\n</template>\n\n<script>\n\nimport axios from 'axios'\nimport swal from 'sweetalert';\n\nexport default {\n  data(){\n    return{\n      form:{\n        _id: '',\n        ItemName:'',\n        __v: \"\"\n      },\n      users: [],\n      updateSubmit: false\n    }\n  },\n\n  methods:{\n    load(){\n      axios.get('http://10.10.131.46:3000/get-all')\n      .then((res)=>{\n        this.users = res.data\n        console.log(res.data)\n      })\n      .catch(()=>{\n        alert('eror load data')\n      })\n    },\n\n    save(){\n      axios.post('http://10.10.131.46:3000/create', this.form)\n      .then(()=>{\n        this.load()\n        this.form.ItemName = ''\n        swal(\"Data Tersimpan\");\n      })\n      .catch(()=>{\n        swal(\"Data Error\");\n      })\n    },\n    edit(user){\n      this.updateSubmit = true\n      this.form._id = user._id\n      this.form.ItemName = user.ItemName\n    },\n    update(form){\n      axios.put('http://10.10.131.46:3000/ItemName/' + form._id , {\n        ItemName: this.form.ItemName\n      })\n      .then((res)=>{\n        this.load()\n        this.form._id = ''\n        this.form.ItemName = ''\n        this.updateSubmit = false\n        console.log(res);\n        swal(\"Data Update\");\n      })\n      .catch(()=>{\n        swal(\"Update Error\");\n      })\n    },\n    hapus(user){\n      axios.delete('http://10.10.131.46:3000/ItemName/' + user._id)\n      .then(()=>{\n        this.load()\n        this.form.ItemName = ''\n        swal(\"Data Terhapus\");\n      })\n      .catch(()=>{\n        swal(\"Data Error\");\n      })\n    }\n  },\n\n  mounted() {\n    this.load()\n  }\n}\n\n</script>\n\n<style lang=\"css\" scoped>\n\n</style>"]}]}