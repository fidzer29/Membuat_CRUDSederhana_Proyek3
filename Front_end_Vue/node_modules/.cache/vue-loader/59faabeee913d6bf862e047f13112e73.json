th scope=\"col\">ID</th>\n      <th scope=\"col\">Name</th>\n      <th scope=\"col\">Action</th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr>\n      <td>\n        <li>{{ user.id }} <br>\n        </li>\n      </td>\n      <td>\n        <li>{{ user.name }} <br>\n        </li>\n      </td>\n      <td>\n        <li><button type=\"button\" v-on:click=\"edit(user)\" name=\"button\" class=\"btn btn-success\">Edit</button> ||\n        <button type=\"button\" v-on:click=\"hapus(user)\" name=\"button\" class=\"btn btn-danger\">Delete</button>\n        </li>\n      </td>\n    </tr>\n  </tbody>\n</table>\n</ul>\n\n</template>\n\n<script>\n\nimport axios from 'axios'\n\nexport default {\n  data(){\n    return{\n      form:{\n        id: '',\n        name:''\n      },\n      users: [],\n      updateSubmit: false\n    }\n  },\n\n  methods:{\n    load(){\n      axios.get('http://localhost:3000/users')\n      .then((res)=>{\n        this.users = res.data\n      })\n      .catch(()=>{\n        alert('eror load data')\n      })\n    },\n\n    save(){\n      axios.post('http://localhost:3000/users', this.form)\n      .then(()=>{\n        this.load()\n        this.form.name = ''\n        swal({\n        icon: \"success\",\n        });\n      })\n      .catch(()=>{\n        alert('saving error')\n      })\n    },\n    edit(user){\n      this.updateSubmit = true\n      this.form.id = user.id\n      this.form.name = user.name\n    },\n    update(form){\n      axios.put('http://localhost:3000/users/' + form.id , {\n        name: this.form.name\n      })\n      .then(()=>{\n        this.load()\n        this.form.name = ''\n        this.updateSubmit = false\n        alert('updated...')\n      })\n      .catch(()=>{\n        alert('error update')\n      })\n    },\n    hapus(user){\n      axios.delete('http://localhost:3000/users/' + user.id)\n      .then(()=>{\n        this.load()\n        this.form.name = ''\n        alert('deleted...')\n      })\n      .catch(()=>{\n        alert('deleted error')\n      })\n    }\n  },\n\n  mounted() {\n    this.load()\n  }\n}\n\n</script>\n\n<style lang=\"css\" scoped>\n\n</style>"]}]}